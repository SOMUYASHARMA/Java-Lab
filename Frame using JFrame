public class FrameDemo {
    public static void main(String args[])
    {
        JFrame ob=new JFrame("First Frame");
        ob.setSize(450,400);
        ob.setVisible(true);
        ob.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        
        
    }
}
